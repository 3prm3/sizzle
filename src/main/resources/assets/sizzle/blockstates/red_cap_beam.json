{
  "multipart": [
    {
      "apply": { "model": "sizzle:block/red_cap_beam_center" }
    },
    {
      "when": { "north": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side" }
    },
    {
      "when": { "east": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "y": 90 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "y": 180 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "y": 270 }
    },
    {
      "when": { "up": false, "post": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 270}
    },
    {
      "when": { "up": true, "post": false },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 270}
    },
    {
      "when": { "up": true, "post": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 270}
    },
    {
      "when": { "down": false, "post": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 90 }
    },
    {
      "when": { "down": true, "post": false },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 90 }
    },
    {
      "when": { "down": true, "post": true },
      "apply": { "model": "sizzle:block/red_cap_beam_side", "x": 90 }
    }
  ]
}
